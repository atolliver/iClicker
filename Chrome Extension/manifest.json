{
  "name": "Automatic iClicker",
  "description": "Automatically answers iClicker questions for desired classes",
  "version": "1.0.4",
  "manifest_version": 3,
  "host_permissions": ["https://www.iclicker.com/*"],
  "permissions": ["activeTab", "scripting", "storage"],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://www.iclicker.com/*"]
    }
  ],
  "icons": {
    "16": "images/iClicker_icon_16.png",
    "32": "images/iClicker_icon_32.png",
    "48": "images/iClicker_icon_48.png",
    "128": "images/iClicker_icon_128.png"
  }
}
