{
  "name": "Automatic iClicker",
  "description": "Automatically answers iClicker questions for desired classes",
  "version": "1.0",
  "manifest_version": 3,
  "host_permissions": ["https://www.iclicker.com/*"],
  "permissions": ["activeTab", "scripting", "storage"],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://www.iclicker.com/*"]
    }
  ]
}
